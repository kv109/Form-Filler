{
    "name": "Form Filler",
    "version": "1.1.0",
    "key":"eilhnemnocfpmkhbaajiefnlonnipcdb",
    "manifest_version": 2,
    "description": "Fills web forms for you.",
    "icons": { "1": "autologger_icon.png" },
     "browser_action": {
        "default_icon": "autologger_icon128x128.png",
        "default_popup": "popup/popup.html",
        "default_title": "Form Filler"
      },
    "options_page": "options.html",
    "permissions": [
        "storage"
    ],
    "background": {
        //"scripts": ["constants.js", "functions.js", "background.js"],
        "scripts": ["bundled_background.js"],
        "persistent": false
    },
    "content_scripts": [
        {
          "matches": ["http://*/*", "https://*/*"],
          "js": [ "bundled_formfiller.js" ],
          //"js": [ "tea.js", "functions.js", "autologger.js" ],
          "all_frames": true
        }
      ]
}

